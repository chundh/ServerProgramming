package useDao;
import java.sql.*;

public class GetBoardTest {
    public static void main(String[] args) {
    	BoardDAO boardDAO = new BoardDAO();
    	BoardVO vo = new BoardVO();
    	vo.setSeq(2);
    	BoardVO board = boardDAO.getBoard(vo);
    	System.out.println(board.getSeq() + " 번 게시글의 상세 정보");
    	
    }
}
